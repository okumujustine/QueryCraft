{
  "name": "querycraft",
  "version": "1.0.0",
  "main": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:ui": "npm run dev --workspace apps/ui",
    "dev:backend": "npm run start:dev --workspace apps/backend",
    "dev:electron": "npm run electron:dev --workspace electron",

    "dev:all": "concurrently \"npm run dev:backend\" \"wait-on tcp:3000 && npm run dev:ui\" \"wait-on tcp:6295 && npm run dev:electron\""

  },
